{
  "version": 2,
  "routes": [
    {
      "src": "^/bingo(/.*)?$",
      "dest": "/bingo/index.html"
    },
    {
      "src": "^/lira(/.*)?$",
      "dest": "/lira/index.html"
    },
    {
      "src": "^/mia(/.*)?$",
      "dest": "/mia/index.html"
    },
    {
      "src": "^/lockdown(/.*)?$",
      "dest": "/lockdown/index.html"
    },
    {
      "src": "^/reaper(/.*)?$",
      "dest": "/reaper/index.html"
    }
  ],
  "static": {
    "bingo": "output/bingo/wwwroot",
    "lira": "output/lira/wwwroot",
    "mia": "output/mia/wwwroot",
    "lockdown": "output/lockdown/wwwroot",
    "reaper": "output/reaper/wwwroot"
  }
}
